<?xml version="1.0" encoding="utf-8" ?>
<!-- Views/Auth/RegistrationPage.xaml -->
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Bingie.Views.Auth.RegistrationPage">
	<StackLayout Padding="20">
		<Entry Placeholder="Username" x:Name="UsernameEntry" />
		<Entry Placeholder="Password" x:Name="PasswordEntry" IsPassword="True" />
		<Button Text="Register" Clicked="OnRegisterClicked" />
	</StackLayout>
</ContentPage>
